    "def LongestNonRepeatingChar(s):\n",
    "  charSet = set()\n",
    "  l = 0\n",
    "  res = 0\n",
    "  for r in range(len(s)):\n",
    "    while s[r] in charSet:\n",
    "      charSet.remove(s[l])\n",
    "      l+=1\n",
    "      \n",
    "    charSet.add(s[r])\n",
    "    res = max(res,r-l+1)\n",
    "  return res\n",
    "\n",
    "s = \"abcabcbb\"\n",
    "print(LongestNonRepeatingChar(s))"
   ]
